{
  "manifest_version": 3,
  "name": "Outrage Filter",
  "description": "Filters out content with moral outrage.",
  "version": "1.0",
  "author": "Mark Torres",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
        "matches": ["*://www.reddit.com/*"],
        "js": ["index.js"],
        "css": ["index.css"]
    }
  ]
}
